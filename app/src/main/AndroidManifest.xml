<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.testexam.charlie.tlive">
    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.testexam.charlie.tlive.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.testexam.charlie.tlive.MainActivity"/>
        <activity android:name="com.testexam.charlie.tlive.login.LoginActivity"/>
        <activity android:name="com.testexam.charlie.tlive.join.JoinActivity"/>
        <activity android:name="com.testexam.charlie.tlive.login.SelectActivity"/>
        <activity android:name=".join.OptionalInfoActivity"/>
    </application>

</manifest>